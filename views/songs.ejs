<% include partials/header.ejs %>

<h1><%= title %></h1>
<table class="table table-striped">
	<tr>
		<th>Title</th>
		<th>Artist</th>
		<th>Year</th>
	</tr>
	<% for (var i=0; i<songs.length;i++){ %>
	<tr>
		<td><% songs[i].title %></td>
		<td><% songs[i].artist %></td>
		<td><% songs[i].year %></td>
	</tr>
	<% } %>
</table>
<% include partials/footer.ejs %>